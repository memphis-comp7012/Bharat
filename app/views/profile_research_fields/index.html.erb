<p id="notice"><%= notice %></p>

<h1>Listing Profile Research Fields</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile_research_fields.each do |profile_research_field| %>
      <tr>
        <td><%= link_to 'Show', profile_research_field %></td>
        <td><%= link_to 'Edit', edit_profile_research_field_path(profile_research_field) %></td>
        <td><%= link_to 'Destroy', profile_research_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

